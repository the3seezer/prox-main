{{!-- layouts/base.hbs --}}

<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
      <title>{{ appName }} | Responsive Bootstrap 4 Admin Dashboard Template</title>
      {{> partials/backend/head }}
      {{#block "head"}}
        {{!-- External page head section--}}
      {{/block}}
  </head>
  <body class="box-bg iq-hotel {{bodyClass}} {{#compare dark '===' 'true'}} dark {{/compare}}">
    <!-- loader Start -->
    <div id="loading">
      {{> partials/backend/loader }}
    </div>
    <!-- loader END -->
    <!-- Wrapper Start -->
    <div class="wrapper">
      <div class="container hotel-container">
        {{> partials/backend/header-layout-1 }}
        <div class="content-page">
          {{#block "content"}}
          {{/block}}
        </div>
        {{> partials/backend/footer }}
      </div>
    </div>
    <!-- Wrapper End-->
    
    {{> partials/backend/scripts }}
    {{#block "scripts"}}
    {{/block}}
  </body>
</html>