
<div class="iq-sidebar iq-sidebar-primary" style="background: url({{path}}assets/images/layouts/layout-9/saidbar.png);background-position: center center;background-color:#01041b;
	background-size: cover;">
    <div class="iq-sidebar-logo d-flex justify-content-between">
        <a href="index.html" class="header-logo">
            {{#compare logoName '!==' ''}}
            <img src="{{path}}assets/images/{{logoName}}" class="img-fluid rounded-normal" alt="logo">
            {{else}}
            <img src="{{path}}assets/images/logo.png" class="img-fluid rounded-normal" alt="logo">
            {{/compare}}
        </a>
        <div class="iq-menu-bt-sidebar">
            <div class="iq-menu-bt align-self-center">
                <div class="wrapper-menu">
                <div class="main-circle"><i class="las la-bars"></i></div>
                </div>
            </div>
        </div>
    </div>
    <div class="data-scrollbar" data-scroll="1">
        <nav class="iq-sidebar-menu">
            <ul id="iq-sidebar-toggle" class="iq-menu">
                {{#each defaultMenu}}
                {{#compare @key '==' 'layout9'}}
                {{#each ../sidebarList}}
                {{#compare ../../layout '===' 'layouts/backend/default'}}
                {{#inArray name ../this}}
                <li class="{{#compare ../../active "==" name }}active{{ else }} {{/compare}}">
                    {{#if children}}
                    <a href="{{href}}" class="collapsed" data-toggle="collapse" aria-expanded="false">
                        <i class="{{icon_class}}"></i><span>{{title}}</span>
                        <i class="{{collapse_icon_class}}"></i>
                        <i class="{{expanded_icon_class}}"></i>
                    </a>
                    <ul id="{{name}}" class="iq-submenu collapse" data-parent="#iq-sidebar-toggle">
                        {{#children}}
                        <li class="{{#compare ../../../active "==" name }}active{{ else }} {{/compare}}">
                            {{#if children}}
                            <a href="{{href}}" class="collapsed" data-toggle="collapse" aria-expanded="false">
                                <i class="{{icon_class}}"></i><span>{{title}}</span>
                                <i class="{{collapse_icon_class}}"></i>
                                <i class="{{expanded_icon_class}}"></i>
                            </a>
                            <ul id="{{name}}" class="iq-submenu iq-submenu-data collapse" data-parent="#{{../name}}">
                                {{#children}}
                                <li class="{{#compare ../../../active "==" name }}active{{ else }} {{/compare}}">
                                    <a href="../{{href}}.html">
                                        <i class="{{icon_class}}"></i><span>{{title}}</span>
                                    </a>
                                </li>
                                {{/children}}
                            </ul>
                            {{else}}
                            <a href="../{{href}}.html">
                                <i class="{{icon_class}}"></i><span>{{title}}</span>
                            </a>
                            {{/if}}
                        </li>
                        {{/children}}
                    </ul>
                    {{else}}
                    <a href="../{{href}}.html" class="">
                        <i class="{{icon_class}}"></i><span>{{title}}</span>
                    </a>
                    {{/if}}
                </li>
                {{/inArray}}
                {{else}}
                <li class="{{#compare ../../active "==" name }}active{{ else }} {{/compare}}">
                    {{#if children}}
                    <a href="{{href}}" class="collapsed" data-toggle="collapse" aria-expanded="false">
                        <i class="{{icon_class}}"></i><span>{{title}}</span>
                        <i class="{{collapse_icon_class}}"></i>
                        <i class="{{expanded_icon_class}}"></i>
                    </a>
                    <ul id="{{name}}" class="iq-submenu collapse" data-parent="#iq-sidebar-toggle">
                        {{#children}}
                        <li class="{{#compare ../../../active "==" name }}active{{ else }} {{/compare}}">
                            {{#if children}}
                            <a href="{{href}}" class="collapsed" data-toggle="collapse" aria-expanded="false">
                                <i class="{{icon_class}}"></i><span>{{title}}</span>
                                <i class="{{collapse_icon_class}}"></i>
                                <i class="{{expanded_icon_class}}"></i>
                            </a>
                            <ul id="{{name}}" class="iq-submenu iq-submenu-data collapse" data-parent="#{{../name}}">
                                {{#children}}
                                <li class="{{#compare ../../../active "==" name }}active{{ else }} {{/compare}}">
                                    <a href="../{{href}}.html">
                                        <i class="{{icon_class}}"></i><span>{{title}}</span>
                                    </a>
                                </li>
                                {{/children}}
                            </ul>
                            {{else}}
                            <a href="../{{href}}.html">
                                <i class="{{icon_class}}"></i><span>{{title}}</span>
                            </a>
                            {{/if}}
                        </li>
                        {{/children}}
                    </ul>
                    {{else}}
                    <a href="../{{href}}.html" class="">
                        <i class="{{icon_class}}"></i><span>{{title}}</span>
                    </a>
                    {{/if}}
                </li>
                {{/compare}}
                {{/each}}
                {{/compare}}
                {{/each}}
            </ul>
        </nav>
        <div class="p-3"></div>
    </div>
    </div>